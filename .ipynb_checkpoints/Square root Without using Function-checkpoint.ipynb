    "num = 37\n",
    "\n",
    "low = 1\n",
    "high =num\n",
    "\n",
    "mid =0\n",
    "\n",
    "res=0\n",
    "\n",
    "\n",
    "\n",
    "while(low<=high):\n",
    "    mid = (low + high)//2\n",
    "    if (mid*mid==num):\n",
    "        res = mid\n",
    "        break\n",
    "    elif(mid*mid>num):\n",
    "        high = mid - 1\n",
    "    else:\n",
    "        res = mid\n",
    "        low = mid+1\n",
    "    \n",
    "print(res)\n",
    "    "
